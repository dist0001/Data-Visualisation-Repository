{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 250,
  "height": 195,
  "data": {
    "url": "https://raw.githubusercontent.com/dist0001/Data-Visualisation-Repository/refs/heads/main/A2%20Dashboard/data/completeDf_projected.csv"
  },
  "params": [
    {
      "name": "max_business",
      "value": 3000,
      "bind": {
        "input": "range",
        "min": 100,
        "max": 12000,
        "step": 100,
        "name": "Max Business: ",
        "element": "input[type=range]"
      }
    },
    {
      "name": "max_jobs",
      "value": 200,
      "bind": {
        "input": "range",
        "min": 10,
        "max": 200,
        "step": 10,
        "name": "Max Jobs: ",
        "element": "input[type=range]"
      }
    }
  ],
  "transform": [
    {"filter": "isValid(datum.jobCount) && isValid(datum.businessCount)"},
    {
      "filter": "datum.businessCount <= max_business && datum.jobCount <= max_jobs"
    }
  ],
  "mark": {"type": "circle", "size": 100, "opacity": 0.7},
  "encoding": {
    "x": {
      "field": "businessCount",
      "type": "quantitative",
      "title": "Business Count",
      "scale": {"type": "sqrt"},
      "axis": {"grid": false}
    },
    "y": {
      "field": "jobCount",
      "type": "quantitative",
      "title": "Job Count",
      "scale": {"type": "sqrt"},
      "axis": {"grid": false}
    },
    "color": {"field": "SA2_NAME16", "type": "nominal", "legend": null},
    "tooltip": [
      {"field": "SA2_NAME16", "type": "nominal", "title": "Suburb"},
      {
        "field": "businessCount",
        "type": "quantitative",
        "title": "Business Count"
      },
      {
        "field": "jobCount",
        "type": "quantitative",
        "title": "Job Count"
      }
    ]
  }
}